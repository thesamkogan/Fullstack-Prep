/*
howEqual
is a function
returns a string
returns strictly if the arguments are strictly equal
returns "loosely" if the arguments are loosely equal
returns not equal if the arguments are not equal
*/


function howEqual(a,b) {
	if (a === b) {
  	return "strictly";
  } else if (a == b) {
   	return "loosely";
	} else {
  	return 'not equal';
  }
}




//EXAMPLE TESTS

console.log(howEqual(0,'0'));
// OUTPUT: 'loosely'

console.log(howEqual(3, 9/3));
// OUTPUT: 'strictly'

console.log(howEqual(true, 'truthy'));
// OUTPUT: 'not equal'

console.log(howEqual(false, '0'));
// OUTPUT: 'loosely'

console.log(howEqual(0,''));
// OUTPUT: 'loosely'
