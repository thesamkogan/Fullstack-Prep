/* 
isDivisible
is a function
returns a boolean
returns true if the dividend can be divided by the divisory
returns false if the dividend can not be divided by the divisor 
*/



function isDivisible(num1,num2) {
	if (num1%num2){
  	return false;}
	else {
  	return true
  }}














//EXAMPLE TESTS

console.log(isDivisible(9, 3));
// OUTPUT: true

console.log(isDivisible(9, 4));
// OUTPUT: false


